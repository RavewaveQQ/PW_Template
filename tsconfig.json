{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "strict": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noFallthroughCasesInSwitch": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "paths": {
      "@modals": ["app/modals"],
      "@components": ["app/components"],
      "@page.object": ["app/pages"],
      "@fixtures": ["fixtures"],
      "@test_data": ["test_data"],
      "@step": ["reporter"]
    }
  }
}